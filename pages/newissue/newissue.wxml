<githead url="{{repourl}}"/>

<van-cell-group>
  <van-field
    value="{{ title }}"
    clearable
    label="Title"
    placeholder="Hello World"
    bind:change="onTitleChange"
  />
</van-cell-group>

<van-cell-group>
  <van-field
    value="{{ content }}"
    clearable
    label="Content"
    placeholder="Hello World"
    bind:change="onContentChange"
  />
</van-cell-group>

<view class="margin-left-right">
  <formbutton type="primary" bind:click="onButtonClick" text="Confirm"/>
</view>