<view class="page">
  <view class="page__hd margin-left-right">GitHub Settings</view>

<wux-cell-group>
  <wux-cell hover-class="none">
    <wux-textarea label="Name" placeholder="Hello World" value="{{ username }}" controlled bind:change="onChange" />
  </wux-cell>

  <wux-cell hover-class="none">
    <wux-textarea label="Token" placeholder="Token From GitHub" value="{{ token }}" controlled bind:change="onTokenChange" />
  </wux-cell>
</wux-cell-group>
</view>