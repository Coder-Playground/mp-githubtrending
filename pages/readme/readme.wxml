<view class="page">
  <view class="page__hd">
  <view class="weui-panel weui-panel_access">
  <view class="weui-media-box weui-media-box_text" bindtap="copy" data-text="{{item.url}}" hover-class="navigator-hover">
    <view class="weui-media-box__title">
        {{item.repo}}
    </view>
    <view class="weui-media-box__info">
      <view class="weui-media-box__info__meta"><wux-icon size="13" type="md-create" />  {{item.lang}}</view>
      <view class="weui-media-box__info__meta"><wux-icon size="13" type="md-star" />  {{item.star}}</view>
      <view class="weui-media-box__info__meta"><wux-icon size="13" type="md-git-network" /> {{item.fork}}</view>
    </view>
    <view class="weui-media-box__desc">{{item.desc}}</view>
  </view>
  </view>
  </view>

  <view class="page__bd">
  <wux-wing-blank size="default">
    <van-icon name="comment" /><h6>  Readme</h6>
    <wux-spin nested spinning="{{ spinning }}" tip="Loading ...">
    <wemark md="{{readme}}" link highlight type="wemark"></wemark>
    </wux-spin>
  </wux-wing-blank>

  <fabbutton/>
  </view>
</view>
